<?jelly escape-by-default='true'?>
<j:jelly xmlns:j="jelly:core" xmlns:f="/lib/form">

  <f:entry title="Path" field="path">
    <f:textbox/>
  </f:entry>

  <f:entry>
    <f:repeatableProperty field="secretValues" minimum="1" add="Add a key/value pair"/>
  </f:entry>

  <f:entry title="">
    <div align="right">
      <f:repeatableDeleteButton/>
    </div>
  </f:entry>

  <f:advanced title="Advanced Settings">
    <f:entry field="engineVersion" title="K/V Engine Version">
      <f:select/>
    </f:entry>
    <f:entry field="vaultCredentialId" title="Credential (override job-level)">
      <f:select/>
    </f:entry>
    <f:entry field="vaultNamespace" title="Namespace (override job-level)">
      <f:textbox/>
    </f:entry>
  </f:advanced>

</j:jelly>
